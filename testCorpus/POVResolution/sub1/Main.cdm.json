{
    "$schema": "../../schema.cdm.json",
    "schemaVersion": "0.6.0",
    "imports": [
        {
            "uri": "/foundations.cdm.json"
        },
        {
            "uri": "/POVResolution/Main.cdm.json",
            "moniker": "base"
        },
        {
            "uri": "/POVResolution/sub1/Linked.cdm.json"
        },
        {
            "uri": "/POVResolution/sub1/Linked3.cdm.json"
        }
    ],
    "definitions": [
        {
            "entityName": "Main",
            "extendsEntity": {
                "entityReference": "base/Main",
                "appliedTraits": [
                    {
                        "traitReference": "sampleEntityTrait",
                        "arguments": [
                            {
                                "name": "P1",
                                "value": "P1FromDerived"
                            }
                        ]
                    }
                ]
            },
            "hasAttributes": [
                {
                    "name": "A11",
                    "relationship": "hasA",
                    "dataType": "string"
                },
                {
                    "name": "A12",
                    "relationship": "hasA",
                    "dataType": "string"
                },
                {
                    "relationship": {
                        "relationshipReference": "referencesA",
                        "appliedTraits": [
                            {
                                "traitReference": "referencesA/exhibitsTraits/does.referenceEntity",
                                "arguments": [
                                    {
                                        "name": "addedAttribute",
                                        "value": {
                                            "name": "main2linked3",
                                            "relationship": "hasA",
                                            "dataType": "entityId"
                                        }
                                    }
                                ]
                            }
                        ]
                    },
                    "entity": {
                        "entityReference": "Linked3",
                        "appliedTraits": [
                            {
                                "traitReference": "is.identifiedBy",
                                "arguments": [
                                    "Linked3/(resolvedAttributes)/linkedId"
                                ]
                            }
                        ]
                    }
                }
            ]
        }
    ]
}