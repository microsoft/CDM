{
    "$schema": "../../../../../../../../../schemaDocuments/schema.cdm.json",
    "jsonSchemaSemanticVersion": "1.1.0",
    "imports": [
        {
            "corpusPath": "_allImports.cdm.json"
        }
    ],
    "definitions": [
        {
            "entityName": "Person",
            "extendsEntity": "CdmEntity",
            "hasAttributes": [
                {
                    "name": "FavoriteTerms",
                    "dataType": "string",
                    "description": "I can construct a list on a Type Attribute.",
                    "projection": {
                        "operations": [
                            {
                                "$type": "alterTraits",
                                "condition": "structured",
                                "traitsToAdd": [
                                    "is.dataFormat.mapValue"
                                ]
                            },
                            {
                                "$type": "addArtifactAttribute",
                                "condition": "structured",
                                "newAttribute": {
                                    "name": "Favorite Terms Key",
                                    "dataType": "string",
                                    "appliedTraits": [
                                        "is.dataFormat.mapKey"
                                    ]
                                },
                                "insertAtTop": true
                            },
                            {
                                "$type": "addAttributeGroup",
                                "condition": "structured",
                                "attributeGroupName": "favorite Term Group"
                            },
                            {
                                "$type": "alterTraits",
                                "condition": "structured",
                                "traitsToAdd": [
                                    "is.dataFormat.map"
                                ]
                            },
                            {
                                "$type": "addArtifactAttribute",
                                "condition": "!structured",
                                "newAttribute": {
                                    "name": "Term key",
                                    "dataType": "string"
                                },
                                "insertAtTop": true
                            },
                            {
                                "$type": "arrayExpansion",
                                "condition": "!structured",
                                "startOrdinal": 1,
                                "endOrdinal": 2
                            },
                            {
                                "$type": "renameAttributes",
                                "condition": "!structured",
                                "renameFormat": "{m}_{o}_value",
                                "applyTo": [
                                    "FavoriteTerms"
                                ]
                            },
                            {
                                "$type": "renameAttributes",
                                "condition": "!structured",
                                "renameFormat": "{m}_{o}_key",
                                "applyTo": [
                                    "Term key"
                                ]
                            },
                            {
                                "$type": "alterTraits",
                                "condition": "!structured",
                                "traitsToAdd": [
                                    {
                                        "traitReference": "has.expansionInfo.mapValue",
                                        "arguments": [
                                            {
                                                "name": "expansionName",
                                                "value": "{m}"
                                            },
                                            {
                                                "name": "ordinal",
                                                "value": "{o}"
                                            }
                                        ]
                                    }
                                ],
                                "applyTo": [
                                    "FavoriteTerms"
                                ],
                                "argumentsContainWildcards": true
                            },
                            {
                                "$type": "alterTraits",
                                "condition": "!structured",
                                "traitsToAdd": [
                                    {
                                        "traitReference": "indicates.expansionInfo.mapKey",
                                        "arguments": [
                                            {
                                                "name": "expansionName",
                                                "value": "{a}"
                                            },
                                            {
                                                "name": "ordinal",
                                                "value": "{o}"
                                            }
                                        ]
                                    }
                                ],
                                "applyTo": [
                                    "Term key"
                                ],
                                "argumentsContainWildcards": true
                            },
                            {
                                "$type": "addArtifactAttribute",
                                "condition": "!structured",
                                "newAttribute": {
                                    "name": "number of favorite terms",
                                    "dataType": "integer"
                                }
                            },
                            {
                                "$type": "alterTraits",
                                "condition": "!structured",
                                "traitsToAdd": [
                                    {
                                        "traitReference": "indicates.expansionInfo.count",
                                        "arguments": [
                                            {
                                                "name": "expansionName",
                                                "value": "{a}"
                                            }
                                        ]
                                    }
                                ],
                                "applyTo": "number of favorite terms",
                                "argumentsContainWildcards": true
                            }
                        ],
                        "runSequentially": true
                    }
                }
            ]
        }
    ]
}