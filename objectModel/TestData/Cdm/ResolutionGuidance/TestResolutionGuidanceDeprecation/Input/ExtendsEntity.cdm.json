  "jsonSchemaSemanticVersion": "1.2.0",
  "imports": [
    {
      "corpusPath": "cdm:/foundations.cdm.json"
    },
    {
      "corpusPath": "BaseEntity.cdm.json"
    }
  ],
  "definitions": [
    {
      "entityName": "Entity",
      "extendsEntity": "BaseEntity",
      "extendsEntityResolutionGuidance": {
