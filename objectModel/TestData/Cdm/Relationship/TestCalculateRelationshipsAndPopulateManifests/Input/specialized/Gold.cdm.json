{
    "$schema": "../schema.cdm.json",
    "jsonSchemaSemanticVersion": "1.0.0",
    "imports": [
        {
            "corpusPath": "cdm:/foundations.cdm.json"
        },
        {
            "corpusPath": "/Lead.cdm.json"
        }
    ],
    "definitions": [
        {
            "entityName": "Gold",
            "extendsEntity": "CdmEntity",
            "hasAttributes": [
                {
                    "purpose": "hasA",
                    "dataType": "string",
                    "name": "goldenLeadName"
                },
                {
                    "name": "leadSource",
                    "entity": {
                        "entityReference": "Lead",
                        "appliedTraits": [
                            {
                                "traitReference": "is.identifiedBy",
                                "arguments": [
                                    "Lead/(resolvedAttributes)/leadId"
                                ]
                            }
                        ]
                    },
                    "resolutionGuidance": {
                        "renameFormat": "{m}",
                        "entityByReference": {
                            "allowReference": true,
                            "foreignKeyAttribute": {
                                "purpose": "hasA",
                                "dataType": "entityId",
                                "name": "leadSourceId"
                            }
                        }
                    }
                }
            ]
        }
    ]
}