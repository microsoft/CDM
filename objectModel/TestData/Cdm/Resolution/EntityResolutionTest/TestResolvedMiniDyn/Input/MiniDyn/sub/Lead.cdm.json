{
    "$schema": "../../schema.cdm.json",
    "jsonSchemaSemanticVersion": "1.0.0",
    "imports": [
        {
            "corpusPath": "_allImports.cdm.json"
        }
    ],
    "definitions": [
        {
            "entityName": "Lead",
            "extendsEntity": "Base",
            "exhibitsTraits": [
                {
                    "traitReference": "perfStandard",
                    "arguments": [
                        {
                            "name": "P1",
                            "value": "P1FromLead"
                        }
                    ]
                },
                {
                    "traitReference": "is.localized.displayedAs",
                    "arguments": [
                        {
                            "name": "localizedDisplayText",
                            "value": {
                                "entityReference": {
                                    "entityShape": "localizedTable",
                                    "constantValues": [
                                        [
                                            "en",
                                            "EntityDisplay"
                                        ]
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "traitReference": "is.localized.describedAs",
                    "arguments": [
                        {
                            "name": "localizedDisplayText",
                            "value": {
                                "entityReference": {
                                    "entityShape": "localizedTable",
                                    "constantValues": [
                                        [
                                            "en",
                                            "This is example description text"
                                        ]
                                    ]
                                }
                            }
                        }
                    ]
                }
            ],
            "displayName": "EntityDisplay",
            "description": "This is example description text",
            "hasAttributes": [
                {
                    "purpose": "identifiedBy",
                    "dataType": "integer",
                    "name": "leadId"
                },
                {
                    "purpose": "hasA",
                    "dataType": "string",
                    "name": "product"
                },
                {
                    "purpose": "hasA",
                    "dataType": "currency",
                    "name": "amount"
                },
                {
                    "name": "forAccount",
                    "entity": {
                        "entityReference": "Account",
                        "appliedTraits": [
                            {
                                "traitReference": "is.identifiedBy",
                                "arguments": [
                                    "Account/(resolvedAttributes)/accountId"
                                ]
                            }
                        ]
                    },
                    "resolutionGuidance": {
                        "entityByReference": {
                            "allowReference": true,
                            "foreignKeyAttribute": {
                                "purpose": "hasA",
                                "dataType": "integer",
                                "name": "id",
                                "appliedTraits": [
                                    "is.linkedEntity.identifier"
                                ]
                            }
                        },
                        "selectsSubAttribute": {
                            "selects": "all"
                        }
                    }
                }
            ]
        }
    ]
}